  "description": "Javascript BCH Wallet Framework",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint .",
    "test": "node test/index.js && npm run lint",
    "discover:mainnet": "node test/discover_electrum_servers.js fulcrum.fountainhead.cash 50002",
    "discover:testnet": "node test/discover_electrum_servers.js tbch.loping.net 60002",
