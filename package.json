  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "docdash": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^30.3.2",
    "jsdoc": "^3.6.5",
    "serve": "^11.3.2"
  },
  "dependencies": {
